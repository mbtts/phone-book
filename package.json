{
  "name": "phone-book",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:mbtts/phone-book.git",
  "author": "mbtts <matthewbetts@me.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-gmaps": "^1.9.0",
    "react-router-dom": "^4.3.1",
    "react-svg-inline": "2.0.1"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.9.0",
    "parcel-plugin-inlinesvg": "^0.0.14",
    "postcss-modules": "^1.1.0",
    "prop-types": "^15.6.1"
  },
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --public-url /",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy": "rm -rf dist && yarn build && firebase deploy"
  },
  "jest": {
    "setupFiles": [
      "./src/configureEnzyme.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^(.*)/.*\\.svg": "<rootdir>/$1/svg.js"
    }
  }
}
